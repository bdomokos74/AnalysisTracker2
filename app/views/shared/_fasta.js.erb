$(document).ready(function() {
    loadFasta();
});

function loadFasta() {
    console.log("called: loadFasta ")
    $.ajax({
        url: '<%= url_for controller: 'fasta_tool', action: 'load_fasta' %>',
        data: "",
        type: 'POST',
        dataType: 'json',
        success: function (data) {
            if(data.status=="OK") {
                console.log("OK")

            } else {
                console.log("status= "+data.status)
            }
        }
    });
}